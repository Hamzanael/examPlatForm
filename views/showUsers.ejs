<%- include('adminPageHaed') -%>


<div class="header">
    <h2>Show students </h2>
</div>

<!-- no change -->
<main>
    <table>
        <thead>
        <tr>
            <th>
                students
            </th>
            <th>
                Email Address
            </th>
            <th>
                  quizzes
            </th>
        </tr>
        </thead>
        <tfoot>
        <tr>
        </tr>
        </tfoot>
        <tbody>
        <%users.forEach(function (user){%>

            <tr>
                <td data-title='Provider Name'>
                 <%=user.name%>
                </td>
                <td data-title='E-mail'>
                    <%=user.username%>
                </td>
                <td class='select'>
                    <%user.quizzes.forEach(function (quiz){%>
                        <%quiz%> ,
                    <%})%>
                </td>
            </tr>

        <%})%>


        </tbody>
    </table>


    </div>
    </div>
</main>
