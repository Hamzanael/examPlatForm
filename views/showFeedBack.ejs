<%- include('adminPageHaed') -%>


<div class="header">
    <h2> feedBack Messages </h2>
</div>

<br>
<br>
<main class="messagesTable">
    <table>
        <thead>
        <tr>
            <th>
                name
            </th>
            <th>
                email
            </th>
            <th>
                feedBack Message
            </th>
        </tr>
        </thead>
        <tfoot>
        <tr>
        </tr>
        </tfoot>
        <tbody>
        <% feedback.forEach(function (feed){ %>
        <tr>
            <td data-title='Provider Name'>
                <%= feed.name %>
            </td>
            <td data-title='E-mail'>
                <%= feed.email %>
            </td>
            <td class='select'>
                <%= feed.massage %>
            </td>
        </tr>
        <% }) %>

        </tbody>
    </table>
</main>

</div>
</div>
</main>
<!-- /////////////////// End Add ///////////  -->
<!-- /////////////////      End table////////////////////////// -->


<script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
